<!--MODIFYING MAIN CODE-->

<mod:findName type="event" name="STORAGE_CHECK_MODULAR">
	<mod-append:choice req="HEKTAR_MODULAR_FOCUS" blue="false" hidden="true">
		<text>Configure a Modular Pinpoint.</text>
		<event load="STORAGE_CHECK_MODULAR_FOCUS"/>
	</mod-append:choice>
	<mod-append:choice req="HEKTAR_MODULAR_SHOTGUN" blue="false" hidden="true">
		<text>Configure a Modular Flak.</text>
		<event load="STORAGE_CHECK_MODULAR_SHOTGUN"/>
	</mod-append:choice>
	<mod-append:choice req="HEKTAR_MODULAR_BOMB" blue="false" hidden="true">
		<text>Configure a Modular Bomb Launcher.</text>
		<event load="STORAGE_CHECK_MODULAR_BOMB"/>
	</mod-append:choice>
</mod:findName>

